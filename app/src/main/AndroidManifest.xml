<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whfp.anti_terrorism">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".basic.BasicApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--主页-->
        <activity android:name=".activity.MainActivity" />
        <!--加油站主页-->
        <activity android:name=".activity.GASActivity" />
        <!--寄递物流主页-->
        <activity android:name=".activity.JDWLActivity" />
        <!--校园主页-->
        <activity android:name=".activity.XYActivity" />
        <!--水电油气主页-->
        <activity android:name=".activity.SDYQActivity" />
        <!--党政机关主页-->
        <activity android:name=".activity.DZJGActivity" />
        <!--化工企业主页-->
        <activity android:name=".activity.HGQYActivity" />
        <!--重点单位主页-->
        <activity android:name=".activity.ZDDWActivity" />
        <!--设置-->
        <activity android:name=".activity.SettingActivity" />
        <!--关于-->
        <activity android:name=".activity.AboutActivity" />
        <!--实时监控查看界面-->
        <activity android:name=".activity.MonitorActivity" />
        <!--历史监控录像列表-->
        <activity android:name=".activity.MonitorHistoryActivity" />
        <!--散装汽油加油报告-->
        <activity android:name=".activity.SZQYJYBGActivity" />
        <!--校车运行情况-->
        <activity android:name=".activity.XCYXQKActivity" />
        <!--校车运行路线-->
        <activity android:name=".activity.BusRouteActivity" />
        <!--乘车人员-->
        <activity android:name=".activity.BusPersonActivity" />
        <!--排污情况-->
        <activity android:name=".activity.PWQKActivity" />
        <!--校车列表-->
        <activity android:name=".activity.SchoolBusListActivity" />
        <!--校园走失情况上报-->
        <activity
            android:name=".activity.XYZSSBActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <!--收寄可疑物品上报-->
        <activity
            android:name=".activity.SJKYWPSBActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <!--案件上报-->
        <activity
            android:name=".activity.AJSBActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <!--可疑情况上报-->
        <activity
            android:name=".activity.KYQKSBActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <!--其他异常情况上报-->
        <activity
            android:name=".activity.QTYCSBActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <!--值班情况上报-->
        <activity
            android:name=".activity.ZBQKSBActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <!--安全防范情况上报-->
        <activity
            android:name=".activity.AQFFSBActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
    </application>

</manifest>